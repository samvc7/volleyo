{
  "name": "volleyo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm prisma generate && pnpm next build",
    "start": "next start",
    "lint": "next lint",
    "db:pull": "dotenv -e .env.local -- npx prisma db pull",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:reset": "dotenv -e .env.local -- npx prisma db push --force-reset",
    "db:reset:test": "prisma db push --skip-generate && ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "e2e": "exec playwright test --headed",
    "e2e:ui": "exec playwright test --ui",
    "e2e:cli": "exec playwright test",
    "e2e:report": "exec playwright show-report"
  },
  "prisma": {
    "seed": "dotenv -e .env.local -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "6.3.0",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.4",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.5",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tanstack/react-table": "^8.20.6",
    "@uidotdev/usehooks": "^2.4.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "15.1.0",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^20.17.16",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.1",
    "eslint": "^9.19.0",
    "eslint-config-next": "15.1.0",
    "postcss": "^8.5.1",
    "prisma": "^6.3.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "react-is": "^19.0.0"
  }
}
